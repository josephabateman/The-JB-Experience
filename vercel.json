{
    "redirects": [
      {
        "source": "/(.*)",
        "destination": "https://thejbexperience.co.uk/$1",
        "permanent": true,
        "has": [
          {
            "type": "host",
            "value": "the-jb-experience.vercel.app"
          }
        ]
      }
    ],
    "headers": [
      {
        "source": "/video/:file",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/x-mpegURL"
          }
        ]
      },
      {
        "source": "/video/:file.ts",
        "headers": [
          {
            "key": "Content-Type",
            "value": "video/MP2T"
          }
        ]
      }
    ]
  }
  